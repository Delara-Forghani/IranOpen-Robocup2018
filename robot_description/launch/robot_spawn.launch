<?xml version="1.0"?>
<launch>
 <arg name="robot_name" default="child4"/>
 <arg name="xcoordinate" default="0.0"/>
 <arg name="ycoordinate" default="0.0"/>
 <group ns="$(arg robot_name)">
  <param name="robot_description" command="$(find xacro)/xacro '$(find robot_description)/urdf/pioneer3at_rc2016.urdf.xacro'"/>
<node pkg="gazebo_ros" type="spawn_model"
          respawn="false"
          name="spawn_model"
          args="-file $(find robot_description)/urdf/model.sdf -sdf -x $(arg xcoordinate) -y $(arg ycoordinate) -model $(arg robot_name)" />

  <node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher">
   <param name="publish_frequency" type="double" value="15.0" />
   <param name="use_gui" value="false"/>
  </node>
   <param name="tf_prefix" type="string" value="$(arg robot_name)"/>
  <node name="robot_state_publisher" pkg="robot_state_publisher"  type="state_publisher">
   <param name="publish_frequency" type="double" value="15.0" />
   <param name="tf_prefix" type="string" value="$(arg robot_name)"/>
  </node>  
  <node pkg="teleop_twist_keyboard" type="teleop_twist_keyboard.py" name="teleop1"/> 
 </group>

</launch>
